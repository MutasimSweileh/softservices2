	function getoffer($parm) {
		$query = "SELECT  * FROM `offer`";

		$condition = "active = 1";

		if (array_key_exists('id', $parm)) {
			$parm["id"] = (int)$parm["id"];
			$condition .= " and id = $parm[id]";
		}


		if (array_key_exists('name', $parm)) {

		if (!$result) {
			$this -> DBsetError();
			$err = $this -> DBgetError();
			$this -> setError("Error getting categories information. : " . $err["1"]);
			$this -> setErrorAr("حدث خطأ بالنظام برجاء الاتصال بمدير الموقع, getjops.");
			return false;
		}

		$locations = array();

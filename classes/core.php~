			$img["image"] = $columns["image"];
                        $img["name"] = $columns["name"];
                        $img["name_arabic"] = $columns["name_arabic"];
			$img["order"] = $columns["order"];
			$img["active"] = $columns["active"];

			array_push($images, $img);
		}

	function getslider($profpram) {
		$query = "SELECT * FROM `slider`";
		$condition = "active = 1 ";

		if (!empty($profpram['id'])) {
			$profpram['id'] = (int)$profpram['id'];
			$condition .= "and  id = $profpram[id]";
		}

		if ($condition)
			$query .= " WHERE ";
